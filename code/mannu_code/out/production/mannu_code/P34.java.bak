class P34{
	public int[] searchRange(int[] nums, int target) {
		int [] res=new int[2];
		int start=0;
		int end=nums.length-1;
		int mid=0;
		while(start<end){
			mid=(end+start)>>>1;
			if(nums[mid]==target){
				int targetStart=mid;
				int targetEnd=mid;
				while(targetStart>0&&nums[targetStart--]==target);
				while (targetEnd<nums.length&&nums[targetEnd++]==target);
				res[0]=targetStart;
				res[1]=targetEnd;
				return res;
			}
			else if(target<nums[mid]){
				end=mid-1;
			}
			else{
				start=mid+1;
			}
		}

		return [-1,-1];
        
    }
	
}
